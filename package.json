{
  "private": true,
  "name": "nodelib",
  "version": "0.0.0",
  "description": "A collection of standard libraries for Node.js",
  "license": "MIT",
  "engines": {
    "node": ">=16.14.0",
    "pnpm": ">=9.13.2"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "preversion": "pnpm lint:size",
    "clean": "pnpm run --parallel clean",
    "lint:code": "pnpm run --parallel lint:code",
    "lint:size": "pnpm run --parallel lint:size",
    "compile": "tsc -b ./packages --verbose",
    "test": "pnpm --parallel test",
    "build": "pnpm run clean && pnpm run compile && pnpm run lint:code && pnpm run test",
    "watch": "tsc -b ./packages --watch",
    "bench": "pnpm run --recursive --sequential bench"
  },
  "devDependencies": {
    "execa": "9.5.1",
    "hereby": "1.10.0",
    "typescript": "5.6.3"
  }
}
