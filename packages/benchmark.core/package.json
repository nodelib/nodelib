{
  "name": "@nodelib/benchmark.core",
  "version": "0.0.0",
  "description": "A super userful and flexible benchmark runner",
  "license": "MIT",
  "repository": "https://github.com/nodelib/nodelib/tree/master/packages/benchmark.core",
  "keywords": [
    "NodeLib",
    "benchmark",
    "bench"
  ],
  "engines": {
    "node": ">= 6"
  },
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "clean": "rimraf out",
    "lint": "tslint \"src/**/*.ts\" -p . -t stylish",
    "compile": "tsc -p .",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run lint && npm run compile && npm test",
    "watch": "npm run clean && npm run compile -- --sourceMap --watch"
  },
  "dependencies": {
    "execa": "^0.10.0",
    "p-map": "^1.2.0"
  }
}
