{
  "name": "@nodelib/benchmark.runner",
  "version": "0.0.0",
  "description": "A runner of races for @nodelib/benchmark",
  "license": "MIT",
  "repository": "https://github.com/nodelib/nodelib/tree/master/packages/benchmark.runner",
  "keywords": [
    "NodeLib",
    "benchmark",
    "bench",
    "runner"
  ],
  "engines": {
    "node": ">= 6"
  },
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "clean": "rimraf out",
    "lint": "tslint \"src/**/*.ts\" -p . -t stylish",
    "compile": "tsc -b .",
    "compile:watch": "tsc -p . --watch",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run lint && npm run compile && npm test",
    "watch": "npm run clean && npm run compile:watch"
  },
  "dependencies": {
    "@nodelib/benchmark.client": "^1.0.0",
    "@nodelib/benchmark.meter": "^1.0.0",
    "@nodelib/benchmark.queue": "^1.0.0",
    "@nodelib/benchmark.worker": "^1.0.0",
    "compute-quantile": "^1.0.1",
    "execa": "^1.0.0",
    "p-map": "^1.2.0"
  }
}
