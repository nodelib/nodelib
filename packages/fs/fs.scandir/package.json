{
  "name": "@nodelib/fs.scandir",
  "version": "2.1.5",
  "description": "List files and directories inside the specified directory",
  "license": "MIT",
  "repository": "https://github.com/nodelib/nodelib/tree/master/packages/fs/fs.scandir",
  "keywords": [
    "NodeLib",
    "fs",
    "FileSystem",
    "file system",
    "scandir",
    "readdir",
    "dirent"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "files": [
    "out/**",
    "!out/**/*.map",
    "!out/**/*.spec.*"
  ],
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "clean": "rimraf {tsconfig.tsbuildinfo,out}",
    "lint": "eslint \"src/**/*.ts\" --cache",
    "compile": "tsc -b .",
    "compile:watch": "tsc -p . --watch --sourceMap",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run compile && npm run lint && npm test",
    "watch": "npm run clean && npm run compile:watch",
    "bench": "npm run bench:async && npm run bench:sync",
    "bench:async": "npm run bench:async:overhead && npm run bench:async:generic && npm run bench:async:fsl && npm run bench:async:stats",
    "bench:async:overhead": "hyperfine -S 'node .benchmark/shell.async.js' --warmup 100 --min-runs 200 'readdir' 'next'",
    "bench:async:generic": "hyperfine -S 'node .benchmark/shell.async.js' --warmup 100 --min-runs 200 'current' 'next'",
    "bench:async:fsl": "BENCHMARK_FSL=1 npm run bench:async:generic",
    "bench:async:stats": "BENCHMARK_STATS=1 npm run bench:async:generic",
    "bench:sync": "npm run bench:sync:overhead && npm run bench:sync:generic && npm run bench:sync:fsl && npm run bench:sync:stats",
    "bench:sync:overhead": "hyperfine -S 'node .benchmark/shell.sync.js' --warmup 100 --min-runs 200 'readdir' 'next'",
    "bench:sync:generic": "hyperfine -S 'node .benchmark/shell.sync.js' --warmup 100 --min-runs 200 'current' 'next'",
    "bench:sync:stats": "BENCHMARK_STATS=1 npm run bench:sync:generic",
    "bench:sync:fsl": "BENCHMARK_FSL=1 npm run bench:sync:generic"
  },
  "dependencies": {
    "@nodelib/fs.stat": "2.0.5",
    "run-parallel": "^1.2.0"
  },
  "devDependencies": {
    "@nodelib/fs.macchiato": "1.0.4",
    "@types/run-parallel": "^1.1.0",
    "@nodelib/fs.scandir": "npm:@nodelib/fs.scandir@2"
  }
}
